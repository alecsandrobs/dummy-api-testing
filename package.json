{
    "name": "api-e2e-test-serenityjs",
    "version": "1.0.0",
    "description": "API end to end testes with SerenityJS",
    "main": "index.js",
    "scripts": {
        "clean": "rimraf target",
        "lint": "tslint --project tsconfig-lint.json --config ../../tslint.json --format stylish",
        "update": "serenity-bdd update --ignoreSSL",
        "test": "cucumber-js",
        "report": "serenity-bdd run",
        "test:report": "failsafe clean update test report",
        "tags": "cucumber-js --tags \"$TAGS\"",
        "scenario": "cucumber-js --name \"$SCENARIO\"",
        "file": "cucumber-js \"features/requirements/**/$FILE.feature\"",
        "dir": "cucumber-js \"features/requirements/**/$DIR/**/*.feature\"",
        "validate": "npm test"
    },
    "devDependencies": {
        "@cucumber/cucumber": "7.2.1",
        "@serenity-js/assertions": "2.28.1",
        "@serenity-js/console-reporter": "2.28.1",
        "@serenity-js/core": "2.28.1",
        "@serenity-js/cucumber": "2.28.1",
        "@serenity-js/local-server": "2.28.1",
        "@serenity-js/rest": "2.28.1",
        "@serenity-js/serenity-bdd": "2.28.1",
        "@types/js-yaml": "^4.0.1",
        "@types/node": "14.14.35",
        "@types/pg": "8.6.0",
        "ajv": "6.10.2",
        "axios": "0.19.2",
        "chai": "4.2.0",
        "chai-arrays": "2.2.0",
        "chai-as-promised": "7.1.1",
        "chai-fs": "2.0.0",
        "chai-json": "1.0.0",
        "chai-json-pattern": "1.1.0",
        "chai-json-schema-ajv": "5.0.0",
        "dotenv": "8.2.0",
        "express": "^4.17.1",
        "fs": "^0.0.1-security",
        "http-status-codes": "^2.1.3",
        "joi": "17.4.0",
        "js-yaml": "^3.14.0",
        "jsonpath": "1.1.1",
        "npm-failsafe": "^0.4.1",
        "pg": "8.6.0",
        "rimraf": "^3.0.2",
        "tiny-types": "1.16.1",
        "ts-node": "10.0.0",
        "tslint": "6.1.2",
        "typescript": "4.3.2"
    },
    "keywords": [
        "cucumber",
        "rest",
        "serenity-js"
    ],
    "author": "Alecsandro Bez Stolk <alecsandro.stolk@gmail.com>",
    "license": "MIT"
}
